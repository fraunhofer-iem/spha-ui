{
  "name": "spha-visualization",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:demo": "vite --mode demo",
    "build": "vue-tsc -b && vite build",
    "build:demo": "vue-tsc -b && vite build --mode demo",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run src/util src/model",
    "test:components": "vitest run src/components",
    "test:integration": "vitest run src/__tests__/integration.test.ts",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:unit": "vitest run --coverage src/util src/model",
    "test:coverage:components": "vitest run --coverage src/components",
    "test:watch": "vitest --watch",
    "test:run": "vitest run",
    "test:ci": "vitest run --coverage --reporter=junit --reporter=json",
    "test:debug": "vitest --inspect-brk --no-coverage --reporter=verbose",
    "test:performance": "vitest run --reporter=verbose --logHeapUsage",
    "test:large": "vitest run src/**/*.large.test.ts",
    "test:validation": "vitest run src/util/DataValidation.test.ts",
    "test:all": "npm run test:unit && npm run test:components && npm run test:integration",
    "test:quick": "vitest run --passWithNoTests --silent",
    "test:verbose": "vitest run --reporter=verbose"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.5.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "vue": "^3.5.20"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "jsdom": "^26.1.0",
    "sass": "^1.9.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.3",
    "vite-plugin-singlefile": "^2.3.0",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.10"
  }
}
